---
import Head from '@components/Head.astro'
import Navbar from '@components/Navbar.astro'
import '@styles/global.css'

interface Props {
  title?: string
}

const { title = 'Reactor MC' } = Astro.props
---

<!doctype html>
<html lang="en">
  <Head {title} />
  <body class="bg-primary text-white">
    <main class="container mx-auto max-w-7xl py-30">
      <Navbar />
      <slot />
    </main>
  </body>
</html>
